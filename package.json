{
  "name": "metalog",
  "version": "1.1.0",
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "description": "Meta Logger for Metarhia",
  "license": "MIT",
  "keywords": [
    "logger",
    "log",
    "metarhia",
    "impress",
    "jstp",
    "globalstorege"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/metarhia/metalog"
  },
  "main": "metalog.js",
  "readmeFilename": "README.md",
  "files": [],
  "scripts": {
    "test": "npm run -s lint && metatests test/",
    "lint": "eslint . && prettier -c \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\""
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@metarhia/common": "^2.0.0",
    "concolor": "^0.1.12",
    "metastreams": "^0.1.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-metarhia": "^7.0.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "metatests": "^0.7.0",
    "prettier": "^1.18.2"
  }
}
